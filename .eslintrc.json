{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2020": true,
        "jest": true
    },
    "extends": ["standard"],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "indent": ["error", "tab"],
        "semi": ["error", "always"],
        "no-tabs": 0,
        "no-var": 1,
        "no-mixed-spaces-and-tabs": 0
    }
}
